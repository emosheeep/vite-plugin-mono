{
  "name": "vite-plugin-virtual-mpa",
  "version": "1.2.0",
  "description": "Out-of-the-box MPA plugin for Vite, with html template engine and virtual files support.",
  "author": "秦旭洋",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "types/src/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite build --watch",
    "build": "vite build && tsc -d --emitDeclarationOnly",
    "lint": "eslint . --fix --ext .js,.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emosheeep/vite-plugin-virtual-mpa"
  },
  "bugs": {
    "url": "https://github.com/emosheeep/vite-plugin-virtual-mpa"
  },
  "files": [
    "dist",
    "types",
    "CHANGELOG.md",
    "README.md",
    "README.zh_CN.md"
  ],
  "license": "MIT",
  "keywords": [
    "vite",
    "vite-plugin",
    "virtual",
    "html",
    "mpa",
    "multi",
    "multi-page"
  ],
  "dependencies": {
    "@types/connect-history-api-fallback": "^1.3.5",
    "cli-color": "^2.0.3",
    "connect-history-api-fallback": "^2.0.0",
    "ejs": "^3.1.8"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.2",
    "@types/ejs": "^3.1.1",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "rollup-plugin-node-externals": "^5.0.1",
    "typescript": "*",
    "vite": "^3.1.8",
    "vite-plugin-checker": "^0.5.1"
  },
  "peerDependencies": {
    "vite": ">= 2.0.0"
  }
}
